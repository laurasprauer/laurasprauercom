  },
  "bugs": {
    "url": "https://github.com/laurasprauer/laurasprauercom/issues"
  },
  "dependencies": {
    "custom-react-scripts": "^0.2.1",
    "link-react": "^3.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "react-scripts": "1.0.17",
    "webpack": "^3.10.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "esw --ext=js --ext=jsx --fix .",
    "lint:watch": "esw --ext=js --ext=jsx -w .",
    "postinstall": "rm node_modules/.bin/react-scripts && npm install --scripts-version custom-react-scripts",
    "start": "concurrently \"npm run startbuild\" \"npm run lint:watch\"",
    "startbuild": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  }
}
